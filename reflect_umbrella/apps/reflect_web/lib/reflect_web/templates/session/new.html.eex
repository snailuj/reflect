<h1>Log In</h1>

<%= form_for @conn, Routes.session_path(@conn, :create), [as: :session], fn f -> %>
    <div class="form-group">
        <%= text_input f, :email %>
    </div>
    <div class="form-group">
        <%= password_input f, :password %>
    </div>

    <%= submit "Log in", class: "primary" %>
<% end %>